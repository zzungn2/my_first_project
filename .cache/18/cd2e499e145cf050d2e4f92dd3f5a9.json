\r\n// ajax.open('GET', NEWS_URL , false);\r\n// 불러오기\r\n// ajax.send();\r\n// 처리\r\n// const newsFeed = JSON.parse(ajax.response);\r\n\r\n// 중복코드 -> 함수안에 넣기\r\nfunction getData(url) {\r\n    ajax.open('GET', url, false);\r\n    ajax.send();\r\n\r\n    return JSON.parse(ajax.response);\r\n}\r\n\r\nconst newsFeed = getData(NEWS_URL);\r\n\r\n\r\n// 입력\r\nconst ul = document.createElement('ul');\r\n\r\n// 이벤트 - hashchange\r\nwindow.addEventListener('hashchange', function() {\r\n    // console.log('해시가 변경됨'); -> 고유의 값(id)를 #에 추가해줌\r\n    // 데이터 열기, 불러오기, 처리\r\n    // @제외, #제외 \r\n    const id = location.hash.substr(1);\r\n\r\n    // ajax.open('GET', CONTENT_URL.replace('@id', id), false);\r\n    // ajax.send();\r\n    // const newsContent = JSON.parse(ajax.response);\r\n    const newsContent = getData(CONTENT_URL.replace('@id', id));\r\n\r\n    // h1으로 표현\r\n    const title = document.createElement('h1');\r\n    content.appendChild(title);\r\n    title.innerHTML = newsContent.title;\r\n\r\n});\r\n\r\n// li a \r\n// DOM div 사용해 innerHTML 시켜 마크업 구조를 선명하게 드러냄\r\n// ul에 fistElementChild시켜 자식으로 포함시킴  \r\nfor(let i = 0; i < 10; i++) {\r\n    const div = document.createElement('div');\r\n\r\n    div.innerHTML = `\r\n    <li> \r\n        <a href = \"#${newsFeed[i].id}\">\r\n            ${newsFeed[i].title}\r\n                (${newsFeed[i].comments_count})\r\n        </a> \r\n    </li>\r\n    `;\r\n\r\n    ul.appendChild(div.firstElementChild);\r\n}\r\n\r\ncontainer.appendChild(ul);\r\ncontainer.appendChild(content); // 다른 창\r\n"},"lineCount":null}},"error":null,"hash":"95a4baa1d0828e5ef98bddbc327eb613","cacheData":{"env":{}}}